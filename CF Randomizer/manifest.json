{
    "manifest_version":2,
    "name":"Codeforces Randomizer",
    "version":"1.0",
    "description":"A codeforces extension to give a random question!!",
    "icons":{
        "128":"icon128.png",
        "48":"icon48.png",
        "16":"icon16.png"
    },
    "browser_action":{
        "default_icon":"icon16.png",
        "default_popup":"popup.html"
    },
    "permissions":
        [
            "tabs",
            "contextMenus",
            "notifications"
        ]
}